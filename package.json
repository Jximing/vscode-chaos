{
  "name": "chaos",
  "displayName": "chaos",
  "description": "Chaos is a vscode plugin to add markdown grammer `` for your english words in markdown.",
  "version": "0.0.3",
  "publisher": "xu",
  "engines": {
    "vscode": "^1.18.0"
  },
  "icon": "https://github.com/MyHerux/vscode-chaos/raw/master/images/logo.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [{
        "command": "extension.add_chaos",
        "title": "Chaos: Add chaos for selection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}